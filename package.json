{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --hot",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "css-loader": "^3.2.0",
    "file-loader": "^4.1.0",
    "node-sass": "^4.12.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.12.0",
    "vue-router": "^3.1.1",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-logger": "^1.5.3",
    "vuelidate": "^0.7.4",
    "vuetable-2": "^2.0.0-beta.4",
    "vuex": "^3.0.1",
    "webpack": "^4.39.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.9.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "airbnb-base"
    ],
    "plugins": [],
    "rules": {
      "import/no-cycle": 0,
      "no-plusplus": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": ["**/*.{js,vue}", "!**/node_modules/**"]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
